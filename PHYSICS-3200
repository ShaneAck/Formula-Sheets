\begin{align*}
    &\hat{U}(t)\ket{\psi(0)} = \ket{\psi(t)},\ \ \hat{U}^\dagger(t)\hat{U}(t) = 1\\
    &\hat{U}(dt) = 1-\frac{i}{\hbar}\hat{H}dt\\
    &\ket{\psi(t)} = e^{-i\hat{H}t/\hbar}\ket{\psi(0)}\\
    &\hat{H} = -\hat{\boldsymbol{\mu}}\cdot\textbf{B} =  \omega_0\hat{S}_n\ \ \text{where}\ \ \textbf{B} = B_0\textbf{k},\ \ \omega_0 = geB_0/2mc\\
    &\hat{U}(t) = e^{-i\hat{H}t/\hbar} = e^{-i\hat{S}_z\phi/\hbar} = \hat{R}(\phi\textbf{k})\ \ \text{where}\ \ \phi = \omega_0t\\
    &\frac{\partial}{\partial t}\braket{A} = \frac{i}{\hbar}\braket{\psi(t)|[\hat{H},\hat{A}]|\psi(t)} + \braket{\psi(t)|\frac{\partial\hat{A}}{\partial t}|\psi(t)}\\
    &\text{MRI:}\ \ \hat{H} = \omega_0\hat{S}_z + \omega_1(\cos{\omega t})\hat{S}_x\\
    &\textbf{B} = B_1\cos{\omega t}\textbf{i} + B_0\textbf{k},\ \ \omega_0 = egB_0/2mc,\ \ \omega_1 = egB_1/2mc\\
    &\text{Energy eigenvalue problem:}\ \ \hat{H}\ket{\psi} = E\ket{\psi}\\
    &\text{Stationary state:}\ \ e^{-i\hat{H}t/\hbar}\ket{E} = e^{-iEt/\hbar}\ket{E}\\
    &\hat{H}\rightarrow\begin{pmatrix} \braket{1|\hat{H}|2} & \braket{1|\hat{H}|2} \\ \braket{2|\hat{H}|1} & \braket{2|\hat{H}|2} \end{pmatrix} = \begin{pmatrix} E_0 & -A \\ -A & E_0 \end{pmatrix}\\
    &\ket{I} = \frac{1}{\sqrt{2}}\ket{1} + \frac{1}{\sqrt{2}},\ \ \ket{II} = \frac{1}{\sqrt{2}}\ket{1} - \frac{1}{\sqrt{2}}\ket{2}\\
    &\hat{x}\ket{x} = x\ket{x}\\
    &\braket{x|x'} = \delta(x-x')\\
    &\int_{-\infty}^\infty dx\delta(x-x') = 1,\ \ \int_{-\infty}^\infty dx\delta(x-x')f(x) = f(x')\\
    &\ket{\psi} = \int_{-\infty}^\infty\ket{x}\braket{x|\psi}dx,\ \ \bra{\psi} = \int_{-\infty}^\infty\bra{x'}\braket{\psi|x'}dx'\\
    &\mathbb{I} = \int_{-\infty}^\infty\ket{x}\bra{x}dx,\ \ \mathbb{I} = \int_{-\infty}^\infty\ket{p}\bra{p}dp\\
    &\psi(x) = \braket{x|\psi},\ \ \braket{\psi|\psi} = \int_{-\infty}^\infty\left|\braket{x|\psi}\right|^2dx = 1\\
    &\hat{T}(dx) = 1-\frac{i}{\hbar}\hat{p}_xdx,\ \ \hat{T}(dx)\ket{x} = \ket{x+dx}\\
    &\hat{T}(a) = e^{-i\hat{p}_xa/\hbar},\ \ \hat{T}(a)\ket{x} = \ket{x+a},\ \ \hat{T}^\dagger\hat{T} = 1\\
    &[\hat{x}, \hat{p}_x] = i\hbar\ \rightarrow\ \Delta x\Delta p_x\ge\frac{\hbar}{2}\\
    &\hat{p}_x = -i\hbar\frac{\partial}{\partial x}\\
    &\hat{H} = \frac{\hat{p}_x^2}{2m} + V(\hat{x})\\
    &\frac{d\braket{p_x}}{dt} = \frac{i}{\hbar}\braket{\psi|[\hat{H},\hat{p}_x]|\psi} = \left\langle-\frac{dV}{dx}\right\rangle\\
    &\braket{x|p} = \frac{1}{\sqrt{2\pi\hbar}}e^{ipx/\hbar}\\
    &\hat{p}_x\ket{\psi} = -i\hbar\int dx'\ket{x'}\frac{\partial}{\partial x'}\braket{x'|\psi}\\
    &\braket{x|\hat{p}_x|\psi} = -i\hbar\frac{\partial}{\partial x}\braket{x|\psi}\\
    &\braket{p|\psi} = \int dx\frac{1}{\sqrt{2\pi\hbar}}e^{-ipx/\hbar}\braket{x|\psi}\\
    &\braket{x|\psi} = \int dp\frac{1}{\sqrt{2\pi\hbar}}e^{ipx/\hbar}\braket{p|\psi}\\
    &p = \frac{h}{\lambda}\\
\end{align*}
\begin{align*}
    &\\
    &\text{Harmonic oscillator:}\ \ \hat{H} = \frac{\hat{p}_x^2}{2m} + \frac{1}{2}m\omega^2\hat{x}^2\\
    &\hat{a} = \sqrt{\frac{m\omega}{2\hbar}}\left(\hat{x}+\frac{i}{m\omega}\hat{p}_x\right),\ \ \hat{a}^\dagger = \sqrt{\frac{m\omega}{2\hbar}}\left(\hat{x} - \frac{i}{m\omega}\hat{p}_x\right)\\
    &[\hat{a}, \hat{a}^\dagger] = 1\\
    &\hat{x} = \sqrt{\frac{\hbar}{2m\omega}}(\hat{a} + \hat{a}^\dagger),\ \ \hat{p}_x = -i\sqrt{\frac{m\omega\hbar}{2}}(\hat{a} - \hat{a}^\dagger)\\
    &\hat{H} = \hbar\omega(\hat{a}^\dagger\hat{a} + \tfrac{1}{2})\\
    &\hat{N} = \hat{a}^\dagger\hat{a}\\
    &[\hat{N}, \hat{a}] = -\hat{a},\ \ [\hat{N}, \hat{a}^\dagger] = \hat{a}^\dagger\\
    &\hat{N}\ket{\eta} = \eta\ket{\eta},\ \ \eta\ge0\\
    &\hat{N}\hat{a}\ket{\eta} = (\eta - 1)\hat{a}\ket{\eta}\ \rightarrow\ \hat{a}\ket{\eta} = c_+\ket{\eta - 1}\\
    &\hat{N}\hat{a}^\dagger\ket{\eta} = (\eta + 1)\hat{a}^\dagger\ket{\eta}\ \rightarrow\ \hat{a}^\dagger\ket{\eta} = c_-\ket{\eta + 1}\\
    &\hat{a}\ket{\eta_\text{min}} = 0\\
    &\hat{N}\ket{n} = n\ket{n},\ \ n = 0, 1, 2,\ldots\\
    &\hat{a}\ket{n} = \sqrt{n}\ket{n-1},\ \ \hat{a}^\dagger\ket{n} = \sqrt{n+1}\ket{n+1}\\
    &\hat{H}\ket{n} = \hbar\omega(\hat{N} + \tfrac{1}{2})\ket{n}\\
    &\ \ \    \indent = \hbar\omega(n + \tfrac{1}{2})\ket{n} = E_n\ket{n},\ \ n = 0, 1, 2, \ldots\\
    &\braket{n'|\hat{a}|n} = \sqrt{n}\delta_{n', n-1},\ \ \braket{n'|\hat{a}^\dagger|n} = \sqrt{n+1}\delta_{n', n+1}\\
    &\ket{n} = \frac{(\hat{a}^\dagger)^n}{\sqrt{n!}}\ket{0}\\
    &\braket{x|\hat{p}_x|0} = -i\hbar\frac{\partial\braket{x|0}}{\partial x}\\
    &\braket{x|0} = \left(\frac{m\omega}{\pi\hbar}\right)^{1/4}e^{-m\omega x^2/2\hbar}\\
    &\braket{x|n} = \frac{1}{\sqrt{n!}}\braket{x|(\hat{a}^\dagger)^n|0}\\
    &\noindent \line(1,0){235}\\
    &\\
    &\text{Identities:}\\
    &\sin^2{\theta} + \cos^2{\theta} = 1\\
    &\sin{(2\theta)} = 2\sin{\theta}\cos{\theta}\\
    &\cos{(2\theta)} = \cos^2{\theta} - \sin^2{\theta}\\
    &\sin{(\theta\pm\phi)} = \sin{\theta}\cos{\phi}\pm\cos{\theta}\sin{\phi}\\
    &\cos{(\theta\pm\phi)} = \cos{\theta}\cos{\phi}\mp\sin{\theta}\sin{\phi}\\
\end{align*}
