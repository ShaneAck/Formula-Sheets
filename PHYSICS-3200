\begin{align*}
    &\text{Midterm:}\\
    &\boldsymbol{\mu} = \frac{q}{2mc}\textbf{L} = \frac{gq}{2mc}\textbf{S}\\
    &F_z = \boldsymbol{\mu}\cdot\frac{\partial\textbf{B}}{\partial z}\simeq\mu_z\frac{\partial B_z}{\partial Z}\\
    &\ket{\psi} = \ket{+\textbf{z}}\braket{+\textbf{z}|\psi} + \ket{-\textbf{z}}\braket{-\textbf{z}|\psi}\\
    &\ket{\psi} = c_+ \ket{+\textbf{z}}+c_-\ket{-\textbf{z}},\ \ \bra{\psi} = c_+^{\ast}\bra{+\textbf{z}}+c_-^{\ast}\bra{-\textbf{z}}\\
    &\left|\braket{\pm\textbf{z}|\psi}\right|^2 \equiv \braket{\pm\textbf{z}|\psi}^{\ast}\braket{\pm\textbf{z}|\psi}\\
    &\ket{\pm\textbf{x}} = \frac{1}{\sqrt{2}}\ket{+\textbf{z}} \pm \frac{1}{\sqrt{2}}\ket{-\textbf{z}}\\
    &\ket{\pm\textbf{y}} = \frac{1}{\sqrt{2}}\ket{+\textbf{z}} \pm \frac{i}{\sqrt{2}}\ket{-\textbf{z}}\\
    &\braket{S_z} = c_+^{\ast}c_+\left(\frac{\hbar}{2}\right)+c_-^{\ast}c_-\left(-\frac{\hbar}{2}\right)\\
    &\Delta S_z = \sqrt{\braket{S_z^2} - \braket{S_z}^2}\\
    &\ket{\psi}\xrightarrow[S_z\text{basis}]{} \begin{pmatrix} \braket{+\textbf{z}|\psi} \\ \braket{-\textbf{z}|\psi} \end{pmatrix} = \begin{pmatrix} c_+ \\ c_- \end{pmatrix}\\
    &\bra{\psi}\xrightarrow[S_z\text{basis}]{}(\braket{\psi|+\textbf{z}},\ \braket{\psi|-\textbf{z}})\\
    &\indent=(\braket{+\textbf{z}|\psi}^{\ast},\ \braket{-\textbf{z}|\psi}^{\ast}) = (c_+^{\ast},\ c_-^{\ast})\\
    &\ket{+\textbf{x}} = \hat{R}(\tfrac{\pi}{2}\textbf{j})\ket{+\textbf{z}},\ \ \bra{+\textbf{x}} = \bra{+\textbf{z}}\hat{R^{\dagger}}(\tfrac{\pi}{2}\textbf{j})\\
    &\braket{i|\hat{A}|j}^\ast = \braket{j|\hat{A}^\dagger|i}\\
    &\hat{A}_{ij}^\dagger = A_{ji}^\ast\\
    &(\hat{A}\hat{B})^\dagger = \hat{B}^\dagger\hat{A}^\dagger\\
    &\text{Unitary:}\ \hat{A}^{\dagger}\hat{A} = \mathbb{I}\\
    &\text{Hermitian/Self-adjoint:}\ \hat{A}^{\dagger} = \hat{A},\ \ \braket{i|\hat{A}|j}^\ast = \braket{j|\hat{A}|i}\\
    &\hat{R}(d\phi\textbf{k}) = 1 - \frac{i}{\hbar}\hat{J}_zd\phi,\ \ \hat{R^{\dagger}}(d\phi\textbf{k}) = 1 + \frac{i}{\hbar}\hat{J}_z^{\dagger}d\phi\\
    &\hat{R}(\phi\textbf{k}) = e^{-i\hat{J}_z\phi/\hbar},\ \ \hat{R}^{\dagger}(\phi\textbf{k}) = e^{i\hat{J}_z\phi/\hbar}\\
    &e^x = \sum_{n=0}^{\infty}\frac{x^n}{n!},\ \ \sin{x} = \sum_{n=0}^{\infty}\frac{(-1)^nx^{2n+1}}{(2n+1)!}\\
    &\cos{x} = \sum_{n=0}^{\infty}\frac{(-1)^nx^{2n}}{(2n)!},\ \ e^{ix} = \cos{x} + i\sin{x}\\
    &\hat{P}_+ = \ket{+\textbf{z}}\bra{+\textbf{z}},\ \ \hat{P}_- = \ket{-\textbf{z}}\bra{-\textbf{z}}\\
    &\mathbb{I}\equiv\ket{+\textbf{z}}\bra{+\textbf{z}} + \ket{-\textbf{z}}\bra{-\textbf{z}}\\
    &\hat{A}\xrightarrow[S_z\text{basis}]{} \begin{pmatrix} \bra{+\textbf{z}}\hat{A}\ket{+\textbf{z}} & \bra{+\textbf{z}}\hat{A}\ket{-\textbf{z}} \\ \bra{-\textbf{z}}\hat{A}\ket{+\textbf{z}} & \bra{-\textbf{z}}\hat{A}\ket{-\textbf{z}} \end{pmatrix} = \begin{pmatrix} A_{11} & A_{12} \\ A_{21} & A_{22} \end{pmatrix}\\
    &\ket{\psi'}\xrightarrow[S_z\text{basis}]{} \begin{pmatrix} \braket{+\textbf{z}|\psi'} \\ \braket{-\textbf{z}|\psi'} \end{pmatrix}\\
    &\indent =\begin{pmatrix} \bra{+\textbf{z}}\hat{R}^{\dagger}(\textstyle\frac{\pi}{2}\textbf{j})\ket{\psi} \\ \bra{-\textbf{z}}\hat{R}^{\dagger}(\textstyle\frac{\pi}{2}\textbf{j})\ket{\psi} \end{pmatrix} = \begin{pmatrix} \braket{+\textbf{x}|\psi} \\ \braket{-\textbf{x}|\psi} \end{pmatrix} \xleftarrow[S_x\text{basis}]{}\ket{\psi}\\
    &\mathbb{S} = \begin{pmatrix} \braket{+\textbf{z}|+\textbf{x}} & \braket{+\textbf{z}|-\textbf{x}} \\ \braket{-\textbf{z}|+\textbf{x}} & \braket{-\textbf{z}|-\textbf{x}} \end{pmatrix}\ \ [S_x\rightarrow S_z\ \text{basis}]\\
    &\hat{A}\xrightarrow[S_x\text{basis}]{}\mathbb{S}^\dagger\mathbb{AS},\ \ \text{where}\ \ \hat{A}\xrightarrow[S_z\text{basis}]{}\mathbb{A}\\
    &[\hat{A}, \hat{B}]\equiv\hat{A}\hat{B}-\hat{B}\hat{A}\\
\end{align*}
\begin{align*}
    &\\
    &\\
    &\braket{S_z} = \braket{\psi|\hat{J}_z|\psi}\\
    &\hat{S}_+\ket{+\textbf{z}} = 0,\ \ \hat{S}_+\ket{-\textbf{z}} = \hbar\ket{+\textbf{z}}\\
    &\hat{S}_-\ket{+\textbf{z}} = \hbar\ket{-\textbf{z}},\ \ \hat{S}_-\ket{-\textbf{z}} = 0\\
    &\hat{J}_z\ket{\pm\textbf{z}} = \pm\frac{\hbar}{2}\ket{\pm\textbf{z}}\\
    &[\hat{J}_x, \hat{J}_y] = i\hbar\hat{J}_z,\ \ [\hat{J}_y, \hat{J}_z] = i\hbar\hat{J}_x,\ \ [\hat{J}_z, \hat{J}_x] = i\hbar\hat{J}_y\\
    &\\
    &[\hat{J}_i,\hat{J}_j] = i\hbar\epsilon_{ijk}\hat{J}_k\\
    &\ \ \epsilon_{ijk} =
    \begin{cases}
        1 & \text{if } ijk\ \text{is any even permutation of } 123\\
        0 & \text{if } i=j,\ i=k,\ \text{or}\ j=k\\
        -1 & \text{if } ijk\ \text{is any odd permutation of } 123
    \end{cases}\\
    &\noindent \line(1,0){235}\\
    &\\
    &\text{Final:}\\
    &[\hat{A}, \hat{B}\hat{C}] = \hat{B}[\hat{A}, \hat{C}] + [\hat{A}, \hat{B}]\hat{C}\\
    &\hat{\textbf{J}} = \hat{J}_x\textbf{i} + \hat{J}_y\textbf{j} + \hat{J}_z\textbf{k},\ \ \hat{\textbf{J}}^2 = \hat{J}_x^2 + \hat{J}_y^2 + \hat{J}_z^2\\
    &\hat{\textbf{J}}^2\ket{\lambda, m} = \lambda\hbar^2\ket{\lambda, m},\ \ \hat{J}_z\ket{\lambda, m} = m\hbar\ket{\lambda, m}\\
    &\hat{J}_\pm = \hat{J}_x \pm i\hat{J}_y\\
    &\hat{J_x} = \frac{\hat{J}_+ + \hat{J}_-}{2},\ \ \hat{J}_y = \frac{\hat{J}_+ - \hat{J}_-}{2i}\\
    &\hat{J}_\pm^\dagger = \hat{J}_\mp,\ \ [\hat{J}_z, \hat{J}_\pm] = \pm\hbar\hat{J}_\pm,\\
    &[\hat{J}_+,\hat{J}_-] = i\hbar\hat{J}_z\\
    &\hat{J}_z\hat{J}_\pm\ket{\lambda, m} = (m\pm1)\hbar\hat{J}_\pm\ket{\lambda, m}\\
    &\hat{\textbf{J}}^2\hat{J}_\pm\ket{\lambda, m} = \lambda\hbar^2\hat{J}_\pm\ket{\lambda, m},\ \ m^2\le\lambda\\
    &\hat{\textbf{J}}^2\ket{j, m} = j(j+1)\hbar^2\ket{j, m},\ \ \hat{J}_z\ket{j, m} = m\hbar\ket{j, m}\\
    &j = 0, \tfrac{1}{2}, 1, \tfrac{3}{2}, 2, \ldots\\
    &m = j, j-1, j-2, \ldots , -j+1, -j\\
    &\text{Magnitude of angular momentum:}\ \ \sqrt{j(j+1)}\hbar\\
    &\text{Projection onto z-axis:}\ \ j\hbar\\
    &\bra{j, m'}\hat{J}_\pm\ket{j, m} = \sqrt{j(j+1)-m(m\pm1)}\hbar\delta_{m', m\pm1}\\
    &\text{where}\ \ \delta_{m', m\pm1} = \braket{j, m'|j, m\pm1}\\    &\braket{\alpha|\alpha}\braket{\beta|\beta}\ge\left|\braket{\alpha|\beta}\right|^2\\
    &[\hat{A}, \hat{B}] = i\hat{C}\ \rightarrow\ \Delta A\Delta B \ge \frac{\left|\braket{C}\right|}{2}\\
    &\Delta J_x\Delta J_y \ge \frac{\hbar}{2}\left|\braket{J_z}\right|\\
    &\hat{\textbf{S}}^2\ket{s,m} = s(s+1)\hbar^2\ket{s,m},\ \ \hat{S}_z\ket{s,m} = m\hbar\ket{s,m}\\
    &s=\tfrac{1}{2},\ \ m=\pm \tfrac{1}{2},\ \ \ket{\tfrac{1}{2}, \tfrac{1}{2}} = \ket{+\textbf{z}},\ \ \ket{\tfrac{1}{2}, -\tfrac{1}{2}} = \ket{-\textbf{z}}\\
    &\hat{\textbf{S}}\rightarrow\frac{\hbar}{2}\boldsymbol{\sigma},\ \ \boldsymbol{\sigma} = \sigma_x\textbf{i} + \sigma_y\textbf{j} + \sigma_z\textbf{k},\\
    &\sigma_x = \begin{pmatrix} 0 & 1 \\ 1 & 0 \end{pmatrix},\ \ \sigma_y = \begin{pmatrix} 0 & -i \\ i & 0 \end{pmatrix},\ \ \sigma_z = \begin{pmatrix} 1 & 0 \\ 0 & -1 \end{pmatrix}\\
    &\hat{S}_n = \hat{\textbf{S}}\cdot\textbf{n}\ \rightarrow\ \hat{S}_n\ket{\mu} = \mu\frac{\hbar}{2}\ket{\mu}\\
\end{align*}
